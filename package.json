{
  "name": "vi-element-vue",
  "version": "1.5.4",
  "private": false,
  "main": "lib/index.umd.min.js",
  "description": "Component library based on Vue",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --mode production",
    "output": "vue inspect > output.js",
    "lib": "vue-cli-service build --target lib --name index --dest lib ./src/index.js"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "markdown-it": "^10.0.0",
    "markdown-it-container": "^2.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.7.0",
    "cheerio": "^1.0.0-rc.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "velocity-animate": "^1.5.2",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "gitHooks": {
    "pre-commit": "npm run lint"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "directories": {
    "lib": "lib"
  }
}
